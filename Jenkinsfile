pipeline { 
    agent any

    stages {
        stage('Build') {
            steps{
                echo 'Building Tic-Tac-Toe game'    
            }
        }
        stage('Test'){
            steps{
                echo 'Testing Tic-Tac-Toe game'    
            }
        }
        stage('Deploy'){
            steps{
                echo 'Deploying Tic-Tac-Toe game'    
            }
        }
    }
}